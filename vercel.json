{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": "nextjs-chatbot/.next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "nextjs-chatbot/src/app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
